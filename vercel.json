{
  "version": 2,
  "builds": [{ "src": "src/index.ts", "use": "@vercel/node" }],

  "routes": [
    { "src": "/(.*)", "dest": "src/index.ts" },
    {
      "src": "/(.*)",
      "dest": "/image",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/image",
      "methods": ["POST"]
    }
  ]
}
